{
  "name": "metal",
  "version": "0.1.0",
  "engines": {
    "node": "20"
  },
  "private": true,
  "scripts": {
    "build:website": "next build",
    "build:worker": "tsc --build ./temporal/tsconfig.json",
    "build": "concurrently --raw \"npm:build:website\" \"npm:build:worker\"",
    "db:push": "drizzle-kit push",
    "db:studio": "bunx drizzle-kit studio",
    "db:nuke": "bun run ./app/server/db/nuke.ts",
    "db:psql": "bun exec 'psql $POSTGRES_URL'",
    "dev:website": "next dev --turbo",
    "dev:otel-collector": "cd hack/opentelemetry-collector-dev-setup && docker-compose up > /dev/null 2>&1",
    "dev:worker": "tsx --tsconfig ./temporal/tsconfig.json --env-file .env --watch --watch-preserve-output ./temporal/src/worker.ts",
    "dev": "concurrently --raw \"npm:dev:*\"",
    "lint": "next lint",
    "start:website": "next start",
    "start:worker": "bunx tsx --tsconfig ./temporal/tsconfig.json ./temporal/src/worker.ts"
  },
